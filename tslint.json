{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "indent": [
            true,
            "spaces",
            4
        ],
        "no-empty": false,
        "no-console": false,
        "interface-name": true,
        "no-var-requires": false,
        "no-trailing-whitespace": false,
        "jsdoc-format": false,
        "max-classes-per-file": false,
        "no-string-literal": false,
        "no-shadowed-variable": false,
        "only-arrow-functions":false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "typeCheck": true,
        "exclude": ["node_modules/**"]
    }
}